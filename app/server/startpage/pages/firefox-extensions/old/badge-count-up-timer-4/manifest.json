{

  "manifest_version": 3,
  "name": "Badge Count Up Timer",
  "version": "1.1.0",
  "description": "Shows a Count Up Timer in extension Badge.",

"icons": {
"512": "icon512x512.png"
},

"permissions":
[
"storage"
],
  

  
"action": {
"default_icon": {              
"512": "icon512x512.png"
    },
    "default_title": "refresh",   
    "default_popup": "popup.html"
  },
  


  
"background": {
"page": "background.html"
}



}
